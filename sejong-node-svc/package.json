{
  "name": "sejong-node-svc",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start": "node src/app.js",
    "db:migrate": "sequelize db:migrate --config src/config/database.js --migrations-path migrations",
    "db:migrate:undo": "sequelize db:migrate:undo --config src/config/database.js --migrations-path migrations",
    "db:seed": "sequelize db:seed:all --config src/config/database.js --seeders-path seeders",
    "db:seed:undo": "sequelize db:seed:undo:all --config src/config/database.js --seeders-path seeders"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "bcryptjs": "^3.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.1",
    "sequelize": "^6.37.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.1"
  }
}
